{
	"version": 2,
	"public": true,
	"builds": [
		{ "src": "index.html", "use": "@vercel/static" }
	],
	"routes": [
		{ "handle": "filesystem" },
		{ "src": "/.*", "dest": "/index.html" }
	],
	"headers": [
		{
			"source": "/(.*)\.css",
			"headers": [
				{ "key": "Content-Type", "value": "text/css; charset=utf-8" },
				{ "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
			]
		}
	]
}
